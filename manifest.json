{
  "manifest_version": 2,
  "name": "Send-to-Discord",
  "version": "1.0",
  "description": "Click to send the link of the opened page into your Discord servers",
  "homepage_url": "https://github.com/usagiga/Send-to-Discord",
  "icons": {
    "48": "assets/icon/48.png",
    "96": "assets/icon/96.png"
  },
  "content_scripts": {
    "matches": ["*://*/*"],
    "js": ["sendToDiscord.js"]
  }
}
